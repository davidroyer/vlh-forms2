{
  "name": "vlh-forms2",
  "version": "0.1.1",
  "private": false,
  "author": {
    "name": "David Royer",
    "email": "droyer01@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davidroyer/vlh-forms2.git"
  },  
  "scripts": {
    "dev": "yarn serve",
    "serve": "vue-cli-service serve --open",
    "serve:example": "vue-cli-service serve ./example --open",
    "build": "vue-cli-service build --name vlh-forms --entry ./src/lib.js --target lib",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "sh deploy-docs.sh"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "vee-validate": "^2.1.3",
    "vue": "^2.5.17",
    "vue-router": "^3.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-plugin-unit-jest": "^3.1.1",
    "@vue/cli-service": "^3.1.4",
    "@vue/eslint-config-prettier": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "lint-staged": "^7.2.2",
    "lodash": "^4.17.11",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-svg-loader": "^0.11.0",
    "vue-template-compiler": "^2.5.17",
    "vuepress": "^1.0.0-alpha.23"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "module": "dist/vlh-forms.common.min.js",
  "main": "dist/vlh-forms.common.min.js",
  "unpkg": "dist/vlh-forms.umd.min.js",
  "jsdelivr": "dist/vlh-forms.umd.min.js",
  "files": [
    "dist/*.css",
    "dist/*.map",
    "dist/*.js",
    "src/*"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }  
}
